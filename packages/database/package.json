{
  "name": "@art-et-jardin/database",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "generate": "dotenv -e ../../.env -- prisma generate",
    "migrate": "dotenv -e ../../.env -- prisma migrate dev",
    "migrate:prod": "dotenv -e ../../.env -- prisma migrate deploy",
    "seed": "dotenv -e ../../.env -- ts-node prisma/seed.ts",
    "reset": "dotenv -e ../../.env -- prisma migrate reset",
    "studio": "dotenv -e ../../.env -- prisma studio",
    "lint": "eslint src --ext .ts"
  },
  "dependencies": {
    "@prisma/client": "^5.8.0",
    "bcrypt": "^5.1.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "dotenv-cli": "^7.4.2",
    "prisma": "^5.8.0",
    "ts-node": "^10.9.0",
    "typescript": "^5.3.0"
  }
}
